{
    "bitwise": true,
    "camelcase": true,
    "trailing": true,
    "eqeqeq": true,
    "curly": true,
    "indent": 4,
    "quotmark": "single",
    "esversion": 6,
    "futurehostile": true,
    "latedef": true,
    "nonew": true,
    "strict": "implied",
    "undef": true,
    "unused": true,
    "expr": true,
    "globals": {
        "require": false,
        "module": false,
        "window": false,
        "document": false,
        "console": false,
        "requestAnimFrame": false,
        "setTimeout": false,
        "clearTimeout": false
    }
}
